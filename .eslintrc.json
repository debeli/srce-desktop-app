{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", "jsx"]
      }
    ],
    "prettier/prettier": ["error"],
    "max-len": ["error", 100],
    "no-console": "off",
    "prefer-destructuring":"off"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
